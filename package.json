{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint .",
    "test": "vitest"
  },
  "dependencies": {
    "@solidjs/router": "^0.5.0",
    "@suid/icons-material": "^0.4.1",
    "@suid/material": "^0.6.3",
    "@unocss/reset": "^0.46.3",
    "solid-js": "^1.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.3",
    "@suid/vite-plugin": "^0.0.3",
    "eslint": "^8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-solid": "^0.8.0",
    "jsdom": "^20.0.2",
    "typescript": "^4.8.4",
    "unocss": "^0.46.3",
    "unplugin-auto-import": "^0.11.4",
    "vite": "^3.2.2",
    "vite-plugin-solid": "^2.4.0",
    "vitest": "^0.25.1"
  },
  "eslintConfig": {
    "plugins": [
      "solid",
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "@antfu",
      "./.eslintrc-auto-import.json",
      "plugin:solid/recommended"
    ],
    "rules": {
      "no-console": "off",
      "solid/jsx-no-undef": "off",
      "react/jsx-indent": [
        2,
        2,
        {
          "checkAttributes": true
        }
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ]
    }
  }
}
